{
  "prefix": "outline",
  "svgoConfig": {
    "plugins": [
      {
        "name": "removeAttrs",
        "params": {
          "attrs": ["stroke", "fill", "color", "width", "height"]
        }
      },
      {
        "name": "addAttributesToSVGElement",
        "params": {
          "attribute": {
            "fill": "none",
            "stroke": "currentColor"
          }
        }
      }
    ]
  }
}
